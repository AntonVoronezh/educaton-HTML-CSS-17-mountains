if title !== 'Мой блог'
  header.header.header-wrap
    .container
      .header__top 
        .header__top-left 
          .socials.socials--header
            - var socials = {'vk':'http://vk.com','github':'http://github.com','in':'http://linkedin.com'}
              each value,key in socials
                a(href=`${value}` target="_blank").socials__link
                  +icon(key, 'socials__img socials__img--header svg', 'ссылка на '+ key)
                  span.visually-hidden ссылка на #{key}
        .header__top-right
          button.btn-hamburger
            +icon('hamburger', 'hamburger  svg', 'меню')
            span.visually-hidden меню
      .header__middle 
        .header__middle-wrap
        .card.card--header
                //- +image('photo/photo.png', 'фото разработчика', 'card__img')
                picture
                  source(media='(min-width:1200px)', srcset='./assets/images/photo/photo-default@1.png 1x,\
                  ./assets/images/photo/photo-default@2.png 2x, ./assets/images/photo/photo-default@3.png 3x')
                  source(media='(min-width:768px)', srcset='./assets/images/photo/photo-desktop@1.png 1x,\
                  ./assets/images/photo/photo-desktop@2.png 2x, ./assets/images/photo/photo-desktop@3.png 3x')
                  source(media='(min-width:480px)', srcset='./assets/images/photo/photo-tablets@1.png 1x,\
                  ./assets/images/photo/photo-tablets@2.png 2x, ./assets/images/photo/photo-tablets@3.png 3x')
                  img.card__img(alt='фото разработчика', src='./assets/images/photo/photo-phones@1.png', srcset='./assets/images/photo/photo-phones@1.png 1x, \
                  ./assets/images/photo/photo-phones@2.png 2x, \
                  ./assets/images/photo/photo-phones@3.png 3x')
                .card__name Владимир Астахов
                .card__desc Личный сайт веб разработчика
      .header__bottom 
        a(href="#").header__bottom-link
          +icon('arrow_down', 'header__img  svg', 'вниз')
          span.visually-hidden дальше

else if title === 'Мой блог'
   header.header.header-wrap
